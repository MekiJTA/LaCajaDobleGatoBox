    <Action Name="A_Switch" Id="{ab8781ab-cba4-40f4-9111-ef8f6611efbe}">
      <Implementation>
        <ST><![CDATA[IF GENERAL.ReadyForAction THEN
	IF Switch1_trig.Q THEN
		nLightsState := 1;
	tempCount2 := tempCount2+1;
	END_IF
	IF Switch2_trig.Q THEN
	nLightsState := 2;
	tempCount3 := tempCount3+1;
	END_IF
END_IF]]></ST>
