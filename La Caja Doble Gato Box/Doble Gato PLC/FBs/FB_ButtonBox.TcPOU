  <POU Name="FB_ButtonBox" Id="{718ab21c-a86f-42b1-ad84-7eea86241524}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_ButtonBox
VAR_INPUT
	RUN		: BOOL;
	iInput1 : BOOL;
	iInput2 : BOOL;
	iInput3 : BOOL;
	nState 	: (IDLE, RUN_FAST, RUN_FASTER, ERROR);
	
END_VAR
VAR_OUTPUT
	Q 		: BOOL;
	oLight1 : BOOL;	
	Fast 	: BOOL;
	Faster 	: BOOL;
END_VAR
VAR
	Button1_trig: R_TRIG;
	Switch1_trig: R_TRIG;
	Switch2_trig: R_TRIG;
	tempCount1: INT;
	tempCount2: INT;
	tempCount3: INT;
	tempCount0: INT;
	
	nLightsState: INT;
	Switch_RS: RS;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[Q := RUN;

A_InputHandling();
A_Switch();

// FB_MotorControl hoitais itse statet ja varmistukset, täällä ohjattais vain valojen vilkuttelua ja nappien kontrollointia

CASE nState OF
		
	
	IDLE:	oLight1 := FALSE;
	
	RUN_FAST:
		oLight1 := GENERAL.BlinkOn;

	RUN_FASTER:
		oLight1 := TRUE;		

	ERROR:
		oLight1 := GENERAL.BlinkFAST_On;

END_CASE]]></ST>
